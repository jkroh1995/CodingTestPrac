import java.util.*;

class Solution {
    public int[] solution(String[] id_list, String[] report, int k) {
        int[] answer = new int[id_list.length];

        //report에서 중복 제거
        HashSet<String> hashset = new HashSet<>(Arrays.asList(report));
        String [] resultReport = hashset.toArray(new String[0]); //resultReport에 중복 제거된 것만 남음

        //새로 배열 형성해서 신고당한 사람 이름만(공백 이후만) 남기기
        String [] Reported = new String[resultReport.length];

        for(int i=0;i<resultReport.length;i++){
            String s = resultReport[i];
            int idx = s.indexOf(" "); //공백의 index를 찾는다.
            Reported[i]=s.substring(idx+1); //index 이후로만 resultReport[i]값을 남긴다.
        }

        //신고 당한 횟수를 담을 배열 생성
        int [] count = new int[id_list.length];

        //id_list의 순서대로 신고 당한 횟수 증가
        for(int i=0;i< id_list.length;i++){
            for(int j=0;j<Reported.length;j++){
                if(id_list[i].equals(Reported[j])) {
                    count[i]++;
                }
            }
        }

        //count가 k값을 넘기면 그 사람을 신고한 사람을 answer에 증가

        for(int i=0;i<count.length;i++){
            if(count[i]>=k){
                for(int j=0;j< resultReport.length;j++){
                    String s = resultReport[j];
                    int idx = s.indexOf(" ");
                    String reporting_name=s.substring(0, idx); //신고한 사람 이름
                    String reported_name=s.substring(idx+1); // 신고당한 사람 이름
                    if(reported_name.equals(id_list[i])){
                        for(int t=0;t<id_list.length;t++){
                            if(reporting_name.equals(id_list[t]))
                                answer[t]++;
                        }
                    }
                }
            }
        }



        return answer;
    }
}

//시간 초과가 한 테스트에서 나왔다 십할;
