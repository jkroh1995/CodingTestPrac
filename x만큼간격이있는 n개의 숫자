//x만큼 간격이 있는 n개의 숫자

class Solution {
    public long[] solution(int x, int n) {
        long[] answer = new long[n];
        long temp = x;
        for(int i=0;i<answer.length;i++){
            answer[i]=temp*(i+1);
        }

        return answer;
    }
}

//매개변수 x 가 int 로 주어지니까 long으로 변환시켜서 적용해야 한다.
