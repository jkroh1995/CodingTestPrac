class Solution {
    public int solution(int left, int right) {
        int answer = 0;
        double left_2 =(double)left;
        double right_2 = (double)right;
        double sum=0;
        for(double i = left;i<right+1;i++){
            if(Math.sqrt(i)%1!=0){
                sum+=i;
            }
            else{
                sum-=i;
            }
        }
        answer = (int)sum;
        return answer;
    }
}
